{
    "name": "cat4year/data-migrator",
    "type": "library",
    "description": "Data migrator package for laravel",
    "version": "0.0.1",
    "keywords": [
        "data",
        "content",
        "rows",
        "migration",
        "migrator",
        "laravel",
        "php"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^8.4",
        "brick/varexporter": "^0.6.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "barryvdh/laravel-ide-helper": "*",
        "brainmaestro/composer-git-hooks": "*",
        "dg/bypass-finals": "^1.9",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^9.13",
        "phpmd/phpmd": "^2.15",
        "phpunit/phpunit": "^11.0.1",
        "rector/rector": "^2.0",
        "spatie/laravel-ignition": "*"
    },
    "autoload": {
        "psr-4": {
            "Cat4year\\DataMigrator\\": "src/"
        },
        "files": [
            "src/Helpers/app.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cat4year\\DataMigratorTests\\": "tests/"
        }
    },
    "scripts": {
        "tb": "php ./vendor/bin/testbench",
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php ./vendor/bin/testbench key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php ./vendor/bin/testbench migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"tb serve\" \"tb queue:listen --tries=1\" \"tb pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "post-install-cmd": [
            "@php ./vendor/bin/testbench route:clear",
            "@php ./vendor/bin/testbench optimize:clear"
        ],
        "post-update-cmd": [
            "@php ./vendor/bin/testbench route:clear",
            "@php ./vendor/bin/testbench optimize:clear",
            "@php ./vendor/bin/testbench vendor:publish --tag=laravel-assets --ansi --force",
            "@php ./vendor/bin/testbench view:clear"
        ],
        "ide": [
            "@php ./vendor/bin/testbench ide-helper:generate",
            "@php ./vendor/bin/testbench ide-helper:models -N -M --dir=\"./tests/App/\" --dir=\"./vendor/orchid/platform/src/Platform/Models/\" --dir=\"./vendor/orchid/platform/src/Attachment/\"",
            "@php ./vendor/bin/testbench ide-helper:meta"
        ],
        "pint": [
            "./vendor/bin/pint -v"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ],
        "lint": [
            "@ide",
            "@rector",
            "@pint"
        ],
        "rector": [
            "sh scripts/rector.sh"
        ],
        "prepare": [
            "@ide",
            "@phpstan",
            "@rector",
            "@pint"
        ],
        "prepare-testbench": "@php vendor/bin/testbench package:discover --ansi",
        "test": [
            "@php ./vendor/bin/testbench test -c ./phpunit.xml"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cat4year\\DataMigrator\\Providers\\DataMigratorServiceProvider"
            ],
            "dont-discover": []
        },
        "hooks": {
            "config": {
                "stop-on-failure": [
                    "pre-commit",
                    "pre-push"
                ]
            },
            "pre-commit": [
                "@putenv php=data-migrator-php",
                "docker exec -i @php composer ide",
                "docker exec -i @php composer pint"
            ],
            "pre-push": [
                "@putenv php=data-migrator-php",
                "docker exec -i @php php ./vendor/bin/phpstan analyse -q --no-progress --configuration=\"./phpstan.neon\""
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
